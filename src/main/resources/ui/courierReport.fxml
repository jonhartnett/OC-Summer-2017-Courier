<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.chart.PieChart?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.Font?>
<GridPane hgap="10.0" vgap="10.0" xmlns="http://javafx.com/javafx/8.0.112" xmlns:fx="http://javafx.com/fxml/1" fx:controller="edu.oc.courier.ui.CourierReportController">
  <columnConstraints>
    <ColumnConstraints />
    <ColumnConstraints />
  </columnConstraints>
  <rowConstraints>
    <RowConstraints />
    <RowConstraints />
    <RowConstraints />
  </rowConstraints>
   <children>
      <ComboBox fx:id="couriers" onAction="#update" promptText="Courier" />
      <PieChart fx:id="pickup" title="Amounte picked up on time" GridPane.rowIndex="1" />
      <PieChart fx:id="deliver" title="Amount delivered on time" GridPane.rowIndex="2" />
      <Label fx:id="speed" wrapText="true" GridPane.columnIndex="1" GridPane.rowIndex="1">
         <font>
            <Font size="64.0" />
         </font>
      </Label>
   </children>
</GridPane>
