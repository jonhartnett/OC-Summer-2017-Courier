<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<fx:root alignment="CENTER" hgap="10.0" onMouseClicked="#select"
         style="-fx-background-color: #ffffff; -fx-border-color: #000000;" type="javafx.scene.layout.GridPane"
         vgap="10.0" xmlns="http://javafx.com/javafx/8.0.112" xmlns:fx="http://javafx.com/fxml/1">
    <columnConstraints>
        <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0"/>
        <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0"/>
    </columnConstraints>
    <rowConstraints>
        <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
        <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
        <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
        <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
    </rowConstraints>
    <children>
        <Label alignment="CENTER_RIGHT" text="Ticket" textAlignment="RIGHT"/>
        <Label alignment="CENTER_RIGHT" text="Client" textAlignment="RIGHT" GridPane.rowIndex="1"/>
        <Label alignment="CENTER_RIGHT" text="Leave time" textAlignment="RIGHT" GridPane.rowIndex="2"/>
        <Label alignment="CENTER_RIGHT" text="Status" textAlignment="RIGHT" GridPane.rowIndex="3"/>
        <Label fx:id="ticketNumber" GridPane.columnIndex="1"/>
        <Label fx:id="clientName" GridPane.columnIndex="1" GridPane.rowIndex="1"/>
        <Label fx:id="leaveTime" GridPane.columnIndex="1" GridPane.rowIndex="2"/>
        <Label fx:id="status" GridPane.columnIndex="1" GridPane.rowIndex="3"/>
    </children>
    <padding>
        <Insets bottom="10.0" left="10.0" right="10.0" top="10.0"/>
    </padding>
</fx:root>
