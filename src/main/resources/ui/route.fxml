<?xml version="1.0" encoding="UTF-8"?>


<?import javafx.geometry.Insets?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.HBox?>
<fx:root type="HBox" xmlns="http://javafx.com/javafx/8.0.112" xmlns:fx="http://javafx.com/fxml/1">
    <children>
        <Label fx:id="first" text="Label">
            <HBox.margin>
                <Insets right="15.0"/>
            </HBox.margin>
        </Label>
        <Label fx:id="last" text="Label">
            <HBox.margin>
                <Insets right="30.0"/>
            </HBox.margin>
        </Label>
        <ComboBox fx:id="condition" promptText="Condition"/>
    </children>
</fx:root>
